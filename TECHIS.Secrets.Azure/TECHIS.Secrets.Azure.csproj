<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
    <Nullable>enable</Nullable>


	  <Description>Quick access to Azure Key vault. Use this to replace "User Secrets"</Description>
	  <Version>1.0.1</Version>
	  <PackageReleaseNotes>Version 1</PackageReleaseNotes>
	  <Authors>TECH-IS, Arinze</Authors>
	  <Company>TECH-IS INC.</Company>
	  <Copyright>TECH-IS INC. 2022</Copyright>
	  <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
	  <PackageProjectUrl>https://github.com/TechInceptions/TECHIS.Secrets.Azure</PackageProjectUrl>
	  <RepositoryUrl>https://github.com/TechInceptions/TECHIS.Secrets.Azure</RepositoryUrl>
	  <PackageTags>Azure Key vault Simple Techis</PackageTags>
	  <Product>EndApps.com</Product>
	  <RepositoryType>Git</RepositoryType>

	  <PackageOutputPath>C:\Data\PackageRepo</PackageOutputPath>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Azure.Identity" Version="1.8.0" />
    <PackageReference Include="Azure.Security.KeyVault.Secrets" Version="4.4.0" />
  </ItemGroup>

</Project>
